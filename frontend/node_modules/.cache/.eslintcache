0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/seanlongdale/Development/code/phase-3/phase-3-project/frontend/phase-3-frontend/src/index.js",[],[],"/Users/seanlongdale/Development/code/phase-3/phase-3-project/frontend/phase-3-frontend/src/App.js",["90","91"],[],"/Users/seanlongdale/Development/code/phase-3/phase-3-project/frontend/phase-3-frontend/src/reportWebVitals.js",[],[],[],"/Users/seanlongdale/Development/code/phase-3/phase-3-project/frontend/phase-3-frontend/src/Exercise.js",[],[],"/Users/seanlongdale/Development/code/phase-3/phase-3-project/frontend/phase-3-frontend/src/PersonalRecords.js",["92","93"],[],"/Users/seanlongdale/Development/code/phase-3/phase-3-project/frontend/phase-3-frontend/src/Record.js",["94","95"],[],"import React, { useState, useEffect } from \"react\";\nimport RecordEdit from \"./RecordEdit\";\n\nfunction Record({ record, onRecordDelete, onRecordUpdate, exerciseSearchTerm, categorySearchTerm }){\n    const [exercise, setExercise] = useState([])\n    const [categories, setCategories] = useState([])\n\n    useEffect(() => {\n        fetch(`http://localhost:9292/exercises/${record.exercise_id}`)\n          .then((r) => r.json())\n          .then((resp) => setExercise(resp));\n        fetch(`http://localhost:9292/exercises/${record.exercise_id}/categories`)\n          .then((r) => r.json())\n          .then((resp) => setCategories(resp));\n      }, []);\n\n    function categoryDisplay(){\n        const categoryArray = categories.map(category => {\n            return category.category\n        })\n        return (\n            <td>{categoryArray.join(\", \")}</td>\n        )\n    }\n\n    function metricDisplay(){\n        if (exercise.for_reps){\n            return(\n                <td>{record.metric} reps</td>\n            )\n        } else if (exercise.for_weight){\n            return(\n                <td>{record.metric} lbs</td>\n            )\n        } else if (exercise.for_time){\n            const minutes = Math.floor(record.metric / 60)\n            const seconds = (record.metric % 60)\n            return(\n                <td>{minutes} mins, {seconds} secs</td>\n            )\n        }\n    }\n\n    function handleRecordDelete(){\n        fetch(`http://localhost:9292//personal_records/${record.id}`, {\n                method:\"DELETE\"\n            })\n       onRecordDelete(record.id)\n    }\n\n    function filterCheck(){\n        const categoryArray = categories.map(category => {\n            return category.category\n        })\n\n        if (exerciseSearchTerm === \"\"){\n            if (categorySearchTerm === \"\"){\n                return table()\n            } else if (categoryArray.join().toLowerCase().includes(categorySearchTerm)){\n                return table()\n            }\n        } else if (exercise.exercise.toLowerCase().includes(exerciseSearchTerm)){\n            return table()\n        }\n    }\n\n    function categoryDisplay(){\n        const categoryArray = categories.map(category => {\n            return category.category\n        })\n        return (\n            <td>{categoryArray.join(\", \")}</td>\n        )\n    }\n\n    function table(){\n        return(\n            <tr key={record.id}>\n                <td>{exercise.exercise}</td>\n                {metricDisplay()}\n                <td>{record.date_created.slice(0,10)}</td>\n                {categoryDisplay()}\n                <RecordEdit \n                    record={record}\n                    exercise={exercise}\n                    onRecordUpdate={onRecordUpdate}\n                />\n                <td><button onClick={handleRecordDelete}>delete</button></td>\n            </tr>\n        )\n    }\n\nreturn (\n    <>\n    {filterCheck()}\n    </>\n)\n}\n\nexport default Record;","/Users/seanlongdale/Development/code/phase-3/phase-3-project/frontend/phase-3-frontend/src/UserInfo.js",[],[],"/Users/seanlongdale/Development/code/phase-3/phase-3-project/frontend/phase-3-frontend/src/Login.js",["96"],[],"/Users/seanlongdale/Development/code/phase-3/phase-3-project/frontend/phase-3-frontend/src/RecordList.js",[],[],"/Users/seanlongdale/Development/code/phase-3/phase-3-project/frontend/phase-3-frontend/src/RecordEdit.js",[],[],"/Users/seanlongdale/Development/code/phase-3/phase-3-project/frontend/phase-3-frontend/src/Filter.js",[],[],"/Users/seanlongdale/Development/code/phase-3/phase-3-project/frontend/phase-3-frontend/src/RecordAdd.js",[],[],"/Users/seanlongdale/Development/code/phase-3/phase-3-project/frontend/phase-3-frontend/src/CategoryAdd.js",[],[],"/Users/seanlongdale/Development/code/phase-3/phase-3-project/frontend/phase-3-frontend/src/ExerciseAdd.js",["97","98"],[],"import React, { useState } from \"react\";\nimport Popup from \"reactjs-popup\";\nimport CategoryAdd from \"./CategoryAdd\";\n\nfunction ExerciseAdd({ categories, onExerciseAdd }){\n    const [categoriesToAdd, setCategoriesToAdd] = useState([])\n    const [metricType, setMetricType] = useState('for_reps')\n    const [newExercise, setNewExercise] = useState('')\n\n    function handleCategoriesToAdd(e){\n        if (e.target.value === \"reset\"){\n            setCategoriesToAdd([])\n        } else if (e.target.value === \"category\"){\n            setCategoriesToAdd([...categoriesToAdd])\n        } else {setCategoriesToAdd([...categoriesToAdd, e.target.value])}\n    }\n\n    function clearExerciseAddContents(){\n        setMetricType('')\n        setCategoriesToAdd([])\n        setNewExercise('')\n    }\n\n    function handleExerciseAddSubmit(e){\n        e.preventDefault()\n        let ex\n        let metric = []\n        if (metricType === \"for_reps\"){\n            metric = [\"true\", \"false\", \"false\"]\n        } else if (metricType === \"for_time\"){\n            metric = [\"false\", \"true\", \"false\"]\n        } else {\n            metric = [\"false\", \"false\", \"true\"]\n        }\n\n        fetch(\"http://localhost:9292/exercises\", {\n            method: \"POST\",\n            headers: {\n                \"Content-Type\": \"application/json\",\n            },\n            body: JSON.stringify({\n                exercise: newExercise,\n                for_reps: metric[0],\n                for_time: metric[1],\n                for_weight: metric[2]\n            }),\n        })\n        .then((r) => r.json())\n        .then((newEx) => {\n            onExerciseAdd(newEx)\n            ex = newEx;\n            categoriesToAdd.map(cat =>{\n                categories.map(category =>{\n                    if (category.category === cat){\n                        fetch(\"http://localhost:9292/exercise_categories\", {\n                            method: \"POST\",\n                            headers: {\n                            \"Content-Type\": \"application/json\",\n                            },\n                            body: JSON.stringify({\n                                category_id: category.id,\n                                exercise_id: ex.id\n                            }),\n                        })  \n                        .then((r) => r.json())\n                        //.then((newCatEx) => console.log(newCatEx));\n                    }\n                })\n                \n            })\n        })\n             \n        clearExerciseAddContents()\n    }\n\n    return(\n        <Popup trigger=\n                {<button id=\"add-exercise-btn\">Add Exercise</button>}\n            modal nested>\n            {close => (\n                <div>\n                <form className=\"exercise-add-form\" onSubmit={(e) => {\n                        handleExerciseAddSubmit(e)\n                        close()\n                    }}>\n                    <legend>Add Exercise</legend>\n                    <input type=\"text\" value={newExercise} onChange={e => setNewExercise(e.target.value)} placeholder=\"exercise name...\"/>\n                    <input id=\"form-submit\" type=\"submit\" value=\"Add\" /><br></br>\n                    <select value={metricType} onChange={e => setMetricType(e.target.value)}>\n                        <option value=\"for_reps\">For Reps</option>\n                        <option value=\"for_time\">For Time</option>\n                        <option value=\"for_weight\">For Weight</option>\n                    </select><br></br>\n                    <CategoryAdd \n                        categories={categories} \n                        categoriesToAdd={categoriesToAdd} \n                        handleCategoriesToAdd={handleCategoriesToAdd}\n                    />\n                </form>\n                <button id=\"form-clear\" onClick={clearExerciseAddContents}>Clear</button>\n                </div>\n            )}\n          </Popup>\n    )\n}\n\nexport default ExerciseAdd;","/Users/seanlongdale/Development/code/phase-3/phase-3-project/frontend/phase-3-frontend/src/LoginPage.js",["99"],[],"/Users/seanlongdale/Development/code/phase-3/phase-3-project/frontend/phase-3-frontend/src/NewUser.js",["100"],[],"/Users/seanlongdale/Development/code/phase-3/phase-3-project/frontend/phase-3-frontend/src/EditProfile.js",[],[],{"ruleId":"101","severity":1,"message":"102","line":3,"column":25,"nodeType":"103","messageId":"104","endLine":3,"endColumn":32},{"ruleId":"101","severity":1,"message":"105","line":12,"column":10,"nodeType":"103","messageId":"104","endLine":12,"endColumn":21},{"ruleId":"101","severity":1,"message":"106","line":12,"column":25,"nodeType":"103","messageId":"104","endLine":12,"endColumn":39},{"ruleId":"107","severity":1,"message":"108","line":28,"column":8,"nodeType":"109","endLine":28,"endColumn":10,"suggestions":"110"},{"ruleId":"107","severity":1,"message":"111","line":15,"column":10,"nodeType":"109","endLine":15,"endColumn":12,"suggestions":"112"},{"ruleId":"113","severity":1,"message":"114","line":67,"column":14,"nodeType":"103","messageId":"115","endLine":67,"endColumn":29},{"ruleId":"101","severity":1,"message":"106"